              <p>Condições: <%= proposal.conditions %></p>
              <p>Risco: <%= proposal.user.scores.first.risk %></p>
              <p>Status da Proposta: <%= proposal.accepted %></p>
              <%= link_to 'Detalhes', loan_proposal_path(proposal), class: 'btn btn-login' %>
